cur_dir=$(dirname $0)
if [ -x $cur_dir/functions ]; then
	. $cur_dir/functions
else
	echo "$cur_dir/functions not found"
	exit 1
fi

# Enable QSFP module power

p10_offset=8
pca9555_23=$(gpio_find 23)
pca9555_23p10=$(($pca9555_23 + $p10_offset))
gpio_set $pca9555_23p10 1
